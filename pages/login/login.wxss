.login-container {
  width: 90%;
  margin: 0 auto;
}
.login-container .title-info {
  width: 100%;
  margin-top: 30%;
}
.login-container .title-info .title {
  font-size: 40rpx;
  color: black;
  width: 100%;
}
.login-container .login-from {
  width: 100%;
  margin-top: 10%;
}
.login-container .login-from .from-item {
  padding-bottom: 20rpx;
  border-bottom: 0.5rpx solid #999999;
  margin-bottom: 20px;
}
.login-container .from-constraint {
  width: 100%;
  margin-bottom: 20rpx;
}
.login-container .from-constraint .checkbox-group {
  display: inline-block;
  vertical-align: middle;
  margin-top: -2px;
  margin-bottom: 1px;
}
.login-container .from-constraint .text {
  display: inline-block;
  margin-left: 20rpx;
  color: #999999;
}
